<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="700.0" prefWidth="200.0" spacing="15.0" styleClass="nav-bar" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gabriel.ecomms.NavBarController">
   <padding>
      <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
   </padding>
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="180.0" text="Seoul Soft" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="homeButton" mnemonicParsing="false" onAction="#handleHomeClick" prefHeight="40.0" prefWidth="180.0" text="Home" />
      <Button fx:id="shopButton" mnemonicParsing="false" onAction="#handleShopClick" prefHeight="40.0" prefWidth="180.0" text="Shop" />
      <Button fx:id="cartButton" mnemonicParsing="false" onAction="#handleCartClick" prefHeight="40.0" prefWidth="180.0" text="Cart" />
      <Button fx:id="adminButton" mnemonicParsing="false" onAction="#handleAdminClick" prefHeight="40.0" prefWidth="180.0" text="Admin" />
   </children>
</VBox>